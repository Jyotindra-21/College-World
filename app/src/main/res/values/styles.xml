        <item name="colorAccent">@color/whiteOrange</item>
    </style>

    <style name="ActionbarTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/whiteOrange</item>
        <item name="colorPrimaryDark">@color/orange</item>
        <item name="colorAccent">@color/whiteOrange</item>
    </style>

